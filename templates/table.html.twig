{% extends 'base.html.twig' %}
{% block body %}
<table class="styled-table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Open</th>
            <th>High</th>
            <th>Low</th>
            <th>Close</th>
            <th>Volume</th>
        </tr>
    </thead>
    <tbody>
    {% for item in items %}
        <tr>
            <td>{{item.date}}</td>
            <td>{{item.open}}</td>
            <td>{{item.high}}</td>
            <td>{{item.low}}</td>
            <td>{{item.close}}</td>
            <td>{{item.volume}}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>

{{ render_chart(chart, {'class': 'my-chart'}) }}


{% endblock %}